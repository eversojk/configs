alias c='clear'
alias grep='grep -in --color'
alias h='history'
alias httpd='python2 -m SimpleHTTPServer'
alias jp='python2 -m json.tool'
alias l='ls -CF'
alias la='ls -A'
alias ll='ls -alFh'
alias lt='ls -lrt'
alias slb='screen -r -d leftbot || screen -S leftbot'
alias slt='screen -r -d lefttop || screen -S lefttop'
alias sr='screen -r -d right || screen -S right'
alias xclip='xclip -selection clipboard'
